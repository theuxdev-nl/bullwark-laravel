{
  "name": "theuxdev/laravel-bullwark",
  "description": "Laravel package for Bullwark, adopting the PHP SDK for easy Laravel use.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bullwark\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Paul 'Bull' Brouwer",
      "email": "paul@theuxdev.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Bullwark\\Providers\\BullwarkServiceProvider"
      ],
      "aliases": {
        "Bullwark": "Bullwark\\Facades\\BullwarkFacade"
      }
    }
  },
  "require": {
    "php": ">=7.4",
    "laravel/framework": "^12.41.1",
    "firebase/php-jwt": "^6.11",
    "theuxdev/bullwark-php-sdk": "dev-main"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev"
}
